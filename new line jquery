$(document).ready(function() {
    $('#sform').submit(function() {
        console.log("Form Submitted");
        var serializedData = $(this).serializeArray();
        var formattedData = serializedData.map(function(field) {
            return field.name + ": " + field.value;
        }).join("\n");
        alert(formattedData);
        return false;
    });
});
